{
  "name": "ku-badminton-reservation",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:prod": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "create-super_admin": "node scripts/create-super_admin.js",
    "cleanup-logs": "node scripts/cleanup-logs.js",
    "migrate-students": "node scripts/migrate-students.js",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "cypress:run": "cypress run --reporter spec",
    "cypress:report": "cypress run --reporter json --reporter-options output=cypress/results/results.json && node scripts/generate-test-report.js",
    "test:all": "cypress run --reporter spec --reporter json --reporter-options output=cypress/results/results.json && node scripts/generate-test-report.js",
    "test:recaptcha": "cypress run --spec 'cypress/e2e/auth/recaptcha-simple.cy.ts'",
    "test:recaptcha:open": "cypress open --e2e --browser chrome",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "6.18.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.21.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.4",
    "@types/react-google-recaptcha": "^2.1.9",
    "bcryptjs": "^3.0.3",
    "chart.js": "^4.5.1",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.26",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.544.0",
    "mime": "^4.1.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.11",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.7",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.3",
    "react-google-recaptcha": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/node": "^20.19.27",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "cypress": "^15.8.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.7",
    "postcss": "^8.5.6",
    "prisma": "6.18.0",
    "tailwindcss": "3.4.17",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "preact": ">=10.28.2",
      "fast-xml-parser": ">=5.3.6",
      "minimatch": ">=10.2.1",
      "qs": ">=6.14.2",
      "systeminformation": ">=5.31.0",
      "lodash": ">=4.17.23",
      "ajv": ">=8.18.0"
    }
  }
}